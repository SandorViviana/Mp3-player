    preset Layout = Core::Layout[AlignToRight, AlignToTop];
    preset Bounds = <17,127,779,372>;
    preset OnLoadItem = OnLoadItem;
    preset SlideHandler = ScrollHandler;
    preset ItemHeight = 50;
